           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="site.title">{{ site.title }}</parameter>
        <parameter key="site.description">{{ site.description }}</parameter>
