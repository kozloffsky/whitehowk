{
    "name": "oz/whitehowk",
    "type": "cmf",
    "description": "WhiteHawk Content Management Framework",
    "keywords": [],
    "homepage": "http://whcmf.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Oz",
            "email": "kozloffsky@gmail.com"
        }
    ],

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/propelorm/Propel2"
        }
    ],

    "require":{
        "symfony/console": "2.4.*",
        "symfony/http-foundation": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/dependency-injection":"2.5.*",
        "symfony/yaml":"2.4.*",
        "symfony/config":"2.4.*",
        "symfony/proxy-manager-bridge":"2.5.*",
        "symfony/event-dispatcher":"2.5.*",
        "symfony/http-kernel":"2.5.*",
        "symfony/finder":"2.5.*",
        "twig/twig": "1.*",
        "propel/propel": "dev-master",
        "zendframework/zend-config": "2.3.*"

    },

    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },


    "autoload": {
        "psr-0": { "Oz": "src/" }
    },

    "autoload-dev": {
        "psr-0": { "Oz": "test/" }
    },

    "bin": [
        "bin/wh"
    ],

    "config": {
        "bin-dir": "bin"
    }

}